alter session set current_schema = shopping;
SET heading OFF;
SELECT * FROM PRODUCTS WHERE PRODUCT_ID IN (SELECT ORDER_ITEMS.PRODUCT_ID FROM ORDERS INNER 
JOIN ORDER_ITEMS ON ORDERS.ORDER_ID = ORDER_ITEMS.ORDER_ID INNER JOIN
PRODUCTS ON PRODUCTS.PRODUCT_ID = ORDER_ITEMS.PRODUCT_ID WHERE ORDERS.ORDER_DATE
BETWEEN '01-Dec-2020' AND '05-Dec-2020');
