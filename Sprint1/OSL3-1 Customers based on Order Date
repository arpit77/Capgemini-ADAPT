alter session set current_schema = shopping;
SET heading OFF;
SELECT CUSTOMERS.CUSTOMER_ID, CUSTOMERS.EMAIL_ID, CUSTOMERS.PASSWORD, CUSTOMERS.FIRST_NAME, 
 CUSTOMERS.LAST_NAME, CUSTOMERS.CITY, CUSTOMERS.GENDER, CUSTOMERS."Phone_No" AS Phone_No FROM CUSTOMERS 
 INNER JOIN ORDERS ON ORDERS.CUSTOMER_ID = CUSTOMERS.CUSTOMER_ID 
 WHERE 
 TO_CHAR(ORDER_DATE, 'DD/MM/YY') BETWEEN '20/11/2020' AND '30/11/2020';
