 alter session set current_schema = shopping;
 SET heading OFF;
 SELECT CUSTOMERS.FIRST_NAME, PRODUCTS.PRODUCT_NAME, ORDER_ITEMS.QUANTITY
 FROM CUSTOMERS INNER JOIN ORDERS ON CUSTOMERS.CUSTOMER_ID = ORDERS.CUSTOMER_ID
 INNER JOIN ORDER_ITEMS ON ORDERS.ORDER_ID = ORDER_ITEMS.ORDER_ID INNER JOIN
 PRODUCTS ON ORDER_ITEMS.PRODUCT_ID = PRODUCTS.PRODUCT_ID 
 WHERE ORDERS.ORDER_DATE = '05-Dec-2020'
 AND CUSTOMERS.CUSTOMER_ID = ORDERS.CUSTOMER_ID
 AND ORDERS.ORDER_ID = ORDER_ITEMS.ORDER_ID
 AND ORDER_ITEMS.PRODUCT_ID = PRODUCTS.PRODUCT_ID;
